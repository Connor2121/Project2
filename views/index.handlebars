<div class="poke-wrapper wrapper">
    <ul id="pokedex">
        <li>
            {{#each pokeImage}} {{/each}}
        </li>
    </ul>
</div>
<div id="pokeModal" class="modal fade" role="dialog" style="display: none;">
    <div class="modal-dialog">
        <h4>Bulbasaur</h4>
        <div class="divider"></div>
        <img id="matchPhoto" class="materialboxed" width="200" src="../images/1.png">
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
